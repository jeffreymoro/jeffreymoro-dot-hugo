a {
  border-bottom: none;
  -moz-transition: color .5s ease;
  -webkit-transition: color .5s ease;
  -o-transition: color .5s ease;
  transition: color .5s ease;
  text-decoration: none !important;
  color: #75b5aa; }
  a:hover {
    color: #F2AB9B !important; }

.toc a, p a {
  position: relative;
  padding-bottom: 0px; }
  .toc a:hover, p a:hover {
    text-decoration: none; }
    .toc a:hover:before, p a:hover:before {
      width: 100%;
      background: #F2AB9B;
      transition: width .5s ease; }
    .toc a:hover:after, p a:hover:after {
      width: 100%;
      background: transparent;
      transition: all 0s ease; }
  .toc a:before, p a:before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    bottom: -3px;
    height: 2px;
    width: 0;
    transition: width 0s ease, background .5s ease; }
  .toc a:after, p a:after {
    content: '';
    display: block;
    position: absolute;
    right: 0;
    bottom: -3px;
    height: 2px;
    width: 0;
    background: #F2AB9B;
    transition: width .5s ease; }

.pattern [aria-current]:hover {
  color: #fefefe !important; }

#mailbox {
  color: rgba(255, 255, 255, 0.5);
